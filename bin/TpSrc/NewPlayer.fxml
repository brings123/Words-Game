<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TpSrc.NewPlayerCont">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #172030;" BorderPane.alignment="CENTER">
         <children>
            <JFXButton layoutX="255.0" layoutY="283.0" onAction="#createNewPlayer" prefHeight="79.0" prefWidth="289.0" style="-fx-background-color: #f48723;" text="Play" textFill="WHITE">
               <font>
                  <Font size="38.0" />
               </font>
            </JFXButton>
            <JFXTextField fx:id="username" focusColor="#f48723" labelFloat="true" layoutX="251.0" layoutY="202.0" prefHeight="38.0" prefWidth="298.0" promptText="User Name" unFocusColor="WHITE">
               <font>
                  <Font size="20.0" />
               </font>
            </JFXTextField>
            <JFXButton layoutX="255.0" layoutY="386.0" onAction="#Return" prefHeight="79.0" prefWidth="289.0" style="-fx-background-color: #f48723;" text="Back" textFill="WHITE">
               <font>
                  <Font size="38.0" />
               </font>
            </JFXButton>
            <Label fx:id="error" layoutX="251.0" layoutY="243.0" prefHeight="24.0" prefWidth="298.0" textFill="RED" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
